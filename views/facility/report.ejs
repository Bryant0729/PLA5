<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<title>REPORT</title>


<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">

<!-- jQuery library -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

<!-- Latest compiled JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

<link href='http://fonts.googleapis.com/css?family=Open+Sans:700,400,300' type='text/css' rel='stylesheet' />
<link href='/css/style.css' type='text/css' rel='stylesheet' />
 
</head>
<% include ../newheader %>
<body>
<div class = "container-fluid">
	<div id="myModal" class="modal" style="display:none">
		<!-- Modal content -->
		<div class="modal-content animate">
			<form id="addAsset" action="addAsset" method="POST">
			<label for="dnsname"><b>DNS Name</b></label>
			<input type="text" placeholder="Enter DNS name" name="dnsname" required><br>
			<input type="hidden" id="elX" name="elX" value="">
			<input type="hidden" id="elY" name="elY" value="">
			<input type="hidden" id="facility" name="facility" value="<%=title%>">
			<button type="submit" style="margin-left:30px;">Add Asset</button>
			</form>
		</div>
	</div>
	<div id="assetModal" class="modal" style="display:none">
		<!-- Modal content -->
		<div class="modal-content animate">
			<iframe id="assetFrame" width="100%" src=""></iframe>
			<form id="deleteAsset" action="deleteAsset" method="POST">
				<input type="hidden" id="dname" name="dname" value="">
				<button type="submit" style="margin-left:30px;">Delete</button>
			</form>
		</div>
	</div>

	<table class="table">
		<thead>
			<tr>
				<th scope="col">ID</th>
				<th scope="col">Name</th>
				<th scope="col">Updated At</th>
				<th scope="col">State</th>
				<th scope="col">Type</th>
				<th scope="col">IP</th>
				<th scope="col">Free Disk Space</th>
				<th scope="col">Uptime</th>
				<th scope="col">Idle Time</th>
				<th scope="col">Current User</th>
				<th scope="col">Battery %</th>
				<th scope="col">Charging %</th>
				<th scope="col">CPU %</th>
				<th scope="col">RAM %</th>
			</tr>
		</thead>
		<tbody>
			<% mdfData['table'].forEach(function(endpoint) { %>
				<tr>
					<td><%= endpoint.id %></td>
					<td><%= endpoint.name %></td>
					<td><%= endpoint.updateTime %></td>
					<td><%= endpoint.state %></td>
					<td><%= endpoint.type %></td>
					<td><%= endpoint.ip %></td>
					<td><%= endpoint.freespace %></td>
					<td><%= endpoint.uptime %></td>
					<td><%= endpoint.idletime %></td>
					<td><%= endpoint.currentUser %></td>
					<td><%= endpoint.batteryP %></td>
					<td><%= endpoint.charging %></td>
					<td><%= endpoint.cpu %></td>
					<td><%= endpoint.ram %></td>
				</tr>
			<% }); %>
		</tbody>
	</table>

</body>


</html>